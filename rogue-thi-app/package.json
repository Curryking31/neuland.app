{
  "name": "rogue-thi-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "next dev",
    "prebuild": "npm run compile-scss",
    "compile-scss": "sass --no-source-map --style compressed scss/themes/:public/themes/",
    "build": "next build",
    "start": "next start",
    "android-add": "rm -rf android/ && npx cap add android",
    "android-open": "next build && next export && npx cap sync android && npx cap open android",
    "android-run": "next build && next export && npx cap sync android && npx cap run android",
    "ios-add": "rm -rf ios/ && npx cap add ios",
    "ios-open": "next build && next export && npx cap sync ios && npx cap open ios",
    "ios-run": "next build && next export && npx cap sync ios && npx cap run ios"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "bootstrap": "^4.6.0",
    "cheerio": "^1.0.0-rc.10",
    "dompurify": "^2.3.2",
    "fetch-bypass-cors": "git+https://github.com/neuland-ingolstadt/fetch-bypass-cors.git#v1.0.0",
    "fetch-cookie": "^1.0.0",
    "idb": "^6.1.3",
    "leaflet": "^1.7.1",
    "next": "^11.1.2",
    "postgres-array": "^3.0.1",
    "prop-types": "^15.7.2",
    "qrcode": "^1.4.4",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.1",
    "react-placeholder": "^4.1.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "sass": "^1.41.1",
    "win95.css": "git+https://github.com/neuland-ingolstadt/win95.css.git",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@capacitor/android": "^3.2.3",
    "@capacitor/cli": "^3.2.3",
    "@capacitor/core": "^3.2.3",
    "@capacitor/ios": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.2"
  }
}
